{
  "service": {
    "id": "swarm-manager",
    "name": "swarm-manager",
    "tags": [
      "{{ lookup('env', 'ENVIRONMENT') }}"
    ],
    "address": "{{ lookup('env', 'HOST_IP') }}",
    "port": 2377,
    "checks": [
      {
        "tcp": "{{ lookup('env', 'HOST_IP') }}:2377",
        "interval": "20s",
        "timeout": "1s"
      }
    ]
  }
}
