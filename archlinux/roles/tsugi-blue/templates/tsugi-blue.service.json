{
  "service": {
    "id": "tsugi-blue",
    "name": "tsugi-blue",
    "tags": [
      "{{ lookup('env', 'ENVIRONMENT') }}",
      "tsugi",
      "blue"
    ],
    "address": "{{ lookup('env', 'HOST_IP') }}",
    "port": 9000,
    "checks": [
      {
        "tcp": "{{ lookup('env', 'HOST_IP') }}:9000",
        "interval": "30s",
        "timeout": "1s"
      }
    ]
  }
}
