{
  "service": {
    "id": "web",
    "name": "web",
    "tags": [
      "{{ lookup('env', 'ENVIRONMENT') }}"
    ],
    "address": "{{ lookup('env', 'HOST_IP') }}",
    "port": 80,
    "checks": [
      {
        "tcp": "{{ lookup('env', 'HOST_IP') }}:80",
        "interval": "30s",
        "timeout": "1s"
      }
    ]
  }
}
